<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FireQ_1_1.View.NewTestView"
             xmlns:Properties="clr-namespace:FireQ_1_1.Properties" x:Class="FireQ_1_1.View.NewTestView.NewTestView"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Label Content="{x:Static Properties:Resources.NewTest}" HorizontalAlignment="Left" Margin="350,20,0,0" VerticalAlignment="Top" FontSize="20"/>
        <Button Command="{Binding BackCommand}" Content="{x:Static Properties:Resources.Back}" HorizontalAlignment="Left" Margin="585,378,0,0" VerticalAlignment="Top" Width="76"/>
        <Button Command="{Binding CloseCommand}" Content="{x:Static Properties:Resources.Close}" HorizontalAlignment="Left" Margin="687,378,0,0" VerticalAlignment="Top" Width="76"/>
    </Grid>
</UserControl>
